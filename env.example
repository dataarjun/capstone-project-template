# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# LangSmith Configuration
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=aml-multi-agent-system
LANGSMITH_WORKSPACE=default
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com

# Memory Configuration
MEM0_API_KEY=your_mem0_api_key_here
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_key_here

# Database Configuration
DATABASE_URL=sqlite:///./data/aml_database.db
# For PostgreSQL: postgresql://aml_user:aml_password@localhost:5432/aml_db
# For Supabase PostgreSQL: postgresql://postgres:[password]@[host]:5432/postgres
POSTGRES_URL=postgresql://postgres:[password]@[host]:5432/postgres

# Vector Database Configuration
VECTOR_DB_PATH=./data/kyc_vectordb

# Application Configuration
DEBUG=false
LOG_LEVEL=INFO
SECRET_KEY=your_secret_key_here

# Security Configuration
ALLOWED_HOSTS=localhost,127.0.0.1,0.0.0.0

# Agent Configuration
MAX_CONCURRENT_INVESTIGATIONS=10
INVESTIGATION_TIMEOUT_SECONDS=300

# Monitoring Configuration
ENABLE_METRICS=true
